  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Console API",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/app.js",
      "runtimeVersion": "8.9.3",
      //"args": ["--hookTimeout=900000"],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std",
      "protocol": "inspector"
    }
  ]
}