  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python-service/uds.py",
      "args": ["pmml4s"],
      "console": "integratedTerminal",
      "env": {"PYTHONPATH": "${PYTHONPATH}:${workspaceFolder}/function"}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node-service/app.js",
      //"args": ["../function/projectConfig.json"],
      "env": {"PYTHONPATH": "${PYTHONPATH}:${workspaceFolder}/function"},
      "runtimeVersion": "12.9.1"
    }
  ]
}